BACKEND=http://127.0.0.1.xip.io:3000
PORT=3000
NODE_ENV=development
FRONTEND=http://localhost:8081
UPLOAD_DIR=/tmp

# MYSQL
MYSQL_HOST=192.168.99.100
MYSQL_PORT=3306
